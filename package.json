{
  "name": "1ilsang.dev",
  "packageManager": "pnpm@9.1.1",
  "private": true,
  "version": "0.0.0",
  "author": "1ilsang <1ilsang@naver.com>",
  "description": "https://1ilsang.dev",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint:stylelint": "stylelint --config './.stylelintrc.cjs' './src/**/*.scss'",
    "lint:prettier": "prettier --list-different './src/**/*.{json,html,yml,tsx,ts,js,jsx,scss}'",
    "lint:eslint": "eslint",
    "lint:markdown": "markdownlint --config './.markdownlint.cjs' './_posts/**/*.{md,mdx}'",
    "lint:typeCheck": "tsc --pretty",
    "lint": "pnpm run '/^lint:.*/'",
    "e2e:build": "NEXT_PUBLIC_CI=true next build",
    "e2e:update": "pnpm playwright test --update-snapshots",
    "e2e:report": "pnpm exec playwright show-report",
    "e2e:install": "pnpm exec playwright install --with-deps",
    "e2e:others": "pnpm playwright test --grep-invert /@/",
    "e2e:dom": "pnpm playwright test --grep '@dom-snapshot'",
    "e2e:screen": "pnpm playwright test --grep '@screen-snapshot'",
    "e2e": "pnpm playwright test",
    "clean": "rm -rf .next out node_modules",
    "knip": "knip",
    "prepare": "husky",
    "preinstall": "npx only-allow pnpm"
  },
  "dependencies": {
    "classnames": "^2.5.1",
    "gray-matter": "4.0.3",
    "next": "14.2.3",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-pretty-code": "^0.13.1",
    "rehype-slug": "^6.0.0",
    "rehype-stringify": "^10.0.0",
    "remark-gfm": "^4.0.0",
    "remark-html": "16.0.1",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.0",
    "sass": "1.77.2",
    "unified": "^11.0.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.44.0",
    "@types/node": "20.12.12",
    "@types/react": "18.3.2",
    "@types/react-dom": "18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "autoprefixer": "10.4.19",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "husky": "9.0.11",
    "knip": "^5.9.4",
    "markdownlint": "0.34.0",
    "markdownlint-cli": "0.40.0",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "stylelint": "^16.3.1",
    "stylelint-config-standard-scss": "^13.1.0",
    "tailwindcss": "3.4.3",
    "typescript": "5.4.5"
  }
}
